{
  "name": "gepur-it/request-converter-bundle",
  "description": "Request converter for symfony",
  "type": "symfony-bundle",
  "license": "MIT",
  "version":"5.0.1",
  "authors": [
    {
      "name": "Andrii Yakovlev",
      "email": "yawa20@gmail.com"
    }
  ],
  "keywords": [
    "gepur",
    "symfony",
    "bundle",
    "request",
    "validator",
    "converter"
  ],
  "homepage": "https://github.com/gepur-it/request-converter-bundle",
  "require": {
    "php": ">=7.3",
    "ext-json": "*",
    "symfony/http-kernel": "^5.0",
    "symfony/console": "^5.0",
    "doctrine/annotations": ">=1.6",
    "symfony/validator": "^5.0",
    "symfony/property-access": "^5.0",
    "symfony/dependency-injection": "^5.0",
    "symfony/config": "^5.0"
  },
  "require-dev": {
    "phpunit/phpunit": ">=7.2"
  },
  "conflict": {
    "gepur-it/request-validator-bundle":"*"
  },
  "autoload": {
    "psr-4": {
      "GepurIt\\RequestConverterBundle\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "GepurIt\\RequestConverterBundle\\Tests\\": "Tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
